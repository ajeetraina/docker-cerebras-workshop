# Cerebras API Configuration
# Get your API key from https://cloud.cerebras.ai/
CEREBRAS_API_KEY=your_cerebras_api_key_here

# Cerebras API Base URL (usually doesn't need to be changed)
CEREBRAS_BASE_URL=https://api.cerebras.ai/v1

# Cerebras Chat Model to use
# Recommended models: llama-3.1-70b, llama-3.1-8b
CEREBRAS_CHAT_MODEL=llama-3.1-70b

# Local Agent Configuration
LOCAL_MODEL_NAME=llama-3.2-3b-instruct

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO

# Optional: Custom ports if defaults conflict
# DEVDUCK_PORT=8000
